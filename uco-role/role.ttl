# baseURI: http://dev.caseontology.org/ontology/uco/role/0.4.1
# imports: http://dev.caseontology.org/ontology/uco/core/0.4.1

@base <http://dev.caseontology.org/ontology/uco/role/0.4.1> .
@prefix core: <http://dev.caseontology.org/ontology/uco/core/0.4.1#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix role: <http://dev.caseontology.org/ontology/uco/role/0.4.1#> .
@prefix xs: <http://www.w3.org/2001/XMLSchema#> .

<https://unifiedcyberontology.org/ontology/uco/role>
	a owl:Ontology ;
	rdfs:label "uco-role"@en ;
	owl:imports <http://dev.caseontology.org/ontology/uco/core/0.4.1> ;
	owl:ontologyIRI <https://unifiedcyberontology.org/ontology/uco/role> ;
	owl:versionIRI <http://dev.caseontology.org/ontology/uco/role/0.4.1> ;
	owl:versionInfo "0.4.1" ;
	.

role:BenevolentRole
	a owl:Class ;
	rdfs:subClassOf role:Role ;
	rdfs:label "BenevolentRole"@en ;
	rdfs:comment " A role with postive and/or beneficial intent."@en ;
	.

role:MaliciousRole
	a owl:Class ;
	rdfs:subClassOf role:Role ;
	rdfs:label "MaliciousRole"@en ;
	rdfs:comment " A role with malicious intent."@en ;
	.

role:NeutralRole
	a owl:Class ;
	rdfs:subClassOf role:Role ;
	rdfs:label "NeutralRole"@en ;
	rdfs:comment " A role with neutral intent."@en ;
	.

role:Role
	a owl:Class ;
	rdfs:subClassOf core:UcoObject ;
	rdfs:label "Role"@en ;
	rdfs:comment "Usual or customary function based on contextual perspective."@en ;
	.

