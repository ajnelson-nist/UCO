# baseURI: https://unifiedcyberontology.org/ontology/uco/role
# imports: https://unifiedcyberontology.org/ontology/uco/core

@base <https://unifiedcyberontology.org/ontology/uco/role> .
@prefix xml: <http://www.w3.org/XML/1998/namespace>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix sh: <http://www.w3.org/ns/shacl#>.
@prefix action: <https://unifiedcyberontology.org/ontology/uco/action#>.
@prefix core: <https://unifiedcyberontology.org/ontology/uco/core#>.
@prefix types: <https://unifiedcyberontology.org/ontology/uco/types#>.
@prefix marking: <https://unifiedcyberontology.org/ontology/uco/marking#>.
@prefix identity: <https://unifiedcyberontology.org/ontology/uco/identity#>.
@prefix location: <https://unifiedcyberontology.org/ontology/uco/location#>.
@prefix pattern: <https://unifiedcyberontology.org/ontology/uco/pattern#>.
@prefix role: <https://unifiedcyberontology.org/ontology/uco/role#>.
@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix xs: <http://www.w3.org/2001/XMLSchema#>.


<https://unifiedcyberontology.org/ontology/uco/role> a owl:Ontology ;
    rdfs:label "uco-role"@en ;
    owl:imports <https://unifiedcyberontology.org/ontology/uco/core> .

role:BenevolentRole a owl:Class,
        sh:NodeShape ;
	#from role:Role
	#from core:UcoObject
    sh:property [
        sh:path core:objectMarking
    ] ;
    sh:property [
        sh:path core:type
    ] ;
    sh:property [
        sh:path core:objectCreatedTime ;
        sh:datatype xsd:dateTime ;
        sh:maxCount "1"^^xsd:nonNegativeInteger
    ] ;
    sh:property [
        sh:path core:id ;
        sh:class types:Identifier ;
        sh:maxCount "1"^^xsd:nonNegativeInteger
    ] ;
    sh:property [
        sh:path core:specVersion ;
        sh:datatype xsd:string ;
        sh:maxCount "1"^^xsd:nonNegativeInteger
    ] ;
    sh:property [
        sh:path core:externalReference ;
        sh:class core:ExternalReference ;
        sh:minCount "0"^^xsd:nonNegativeInteger
    ] ;
    sh:property [
        sh:path core:description
    ] ;
    sh:property [
        sh:path core:hasFacet
    ] ;
    sh:property [
        sh:path core:tag
    ] ;
    sh:property [
        sh:path core:createdBy ;
        sh:class core:IdentityAbstraction ;
        sh:maxCount "1"^^xsd:nonNegativeInteger
    ] ;
    sh:property [
        sh:path core:modifiedTime
    ] ;
    sh:property [
        sh:path core:name ;
        sh:datatype xsd:string ;
        sh:maxCount "1"^^xsd:nonNegativeInteger
    ] ;

    rdfs:label "BenevolentRole"@en ;
    rdfs:comment "A benevolent role is a role with positive and/or beneficial intent."@en ;
    rdfs:subClassOf role:Role ;
    sh:targetClass role:BenevolentRole .

